{
  "expo": {
    "name": "Servicio social",
    "slug": "sai-colomos",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "saicolomos",
    "jsEngine": "hermes",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true,
      "bundleIdentifier": "com.saicolomos.saicolomos"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.sai-colomos.dev"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ],
      "package": "com.sai_colomos.app_core"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Necesitamos acceder a tu fotografías para poder seleccionar una."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Necesitamos acceder a tu cámara para poder escanear los códigos QR."
        }
      ]
    ],
    "extra": {
      "environment": "development",
      "api_development": "http://192.168.100.51:3000",
      "api_production": "https://api.sai-colomos.dev",
      "eas": {
        "projectId": "28c70686-5791-4e1a-8272-9f86861b6cad"
      }
    },
    "owner": "sai-colomos"
  }
}
